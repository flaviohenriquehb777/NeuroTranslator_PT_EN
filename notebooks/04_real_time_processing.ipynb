    "class AudioProcessor:\n",
    "    \"\"\"\n",
    "    Classe para processamento de √°udio em tempo real\n",
    "    \"\"\"\n",
    "    def __init__(self, sample_rate=16000, chunk_size=1024, channels=1):\n",
    "        self.sample_rate = sample_rate\n",
    "        self.chunk_size = chunk_size\n",
    "        self.channels = channels\n",
    "        self.format = pyaudio.paInt16\n",
    "        \n",
    "        # Inicializar PyAudio\n",
    "        self.audio = pyaudio.PyAudio()\n",
    "        \n",
    "        # Filas para processamento ass√≠ncrono\n",
    "        self.audio_queue = queue.Queue()\n",
    "        self.text_queue = queue.Queue()\n",
    "        \n",
    "        # Flags de controle\n",
    "        self.is_recording = False\n",
    "        self.is_processing = False\n",
    "        \n",
    "        # Inicializar reconhecedor de fala\n",
    "        self.recognizer = sr.Recognizer()\n",
    "        self.microphone = sr.Microphone()\n",
    "        \n",
    "        # Calibrar ru√≠do ambiente\n",
    "        self._calibrate_microphone()\n",
    "        \n",
    "        print(\"üé§ Sistema de √°udio inicializado!\")\n",
    "    \n",
    "    def _calibrate_microphone(self):\n",
    "        \"\"\"\n",
    "        Calibra o microfone para ru√≠do ambiente\n",
    "        \"\"\"\n",
    "        try:\n",
    "            with self.microphone as source:\n",
    "                print(\"üîß Calibrando microfone... (aguarde 2 segundos)\")\n",
    "                self.recognizer.adjust_for_ambient_noise(source, duration=2)\n",
    "                print(\"‚úÖ Calibra√ß√£o conclu√≠da!\")\n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Erro na calibra√ß√£o: {e}\")\n",
    "    \n",
    "    def start_recording(self):\n",
    "        \"\"\"\n",
    "        Inicia grava√ß√£o cont√≠nua de √°udio\n",
    "        \"\"\"\n",
    "        if self.is_recording:\n",
    "            return\n",
    "        \n",
    "        self.is_recording = True\n",
    "        \n",
    "        # Thread para captura de √°udio\n",
    "        self.recording_thread = threading.Thread(target=self._record_audio)\n",
    "        self.recording_thread.daemon = True\n",
    "        self.recording_thread.start()\n",
    "        \n",
    "        # Thread para processamento de √°udio\n",
    "        self.processing_thread = threading.Thread(target=self._process_audio)\n",
    "        self.processing_thread.daemon = True\n",
    "        self.processing_thread.start()\n",
    "        \n",
    "        print(\"üéôÔ∏è Grava√ß√£o iniciada!\")\n",
    "    \n",
    "    def stop_recording(self):\n",
    "        \"\"\"\n",
    "        Para a grava√ß√£o de √°udio\n",
    "        \"\"\"\n",
    "        self.is_recording = False\n",
    "        self.is_processing = False\n",
    "        print(\"‚èπÔ∏è Grava√ß√£o parada!\")\n",
    "    \n",
    "    def _record_audio(self):\n",
    "        \"\"\"\n",
    "        Captura √°udio do microfone continuamente\n",
    "        \"\"\"\n",
    "        stream = self.audio.open(\n",
    "            format=self.format,\n",
    "            channels=self.channels,\n",
    "            rate=self.sample_rate,\n",
    "            input=True,\n",
    "            frames_per_buffer=self.chunk_size\n",
    "        )\n",
    "        \n",
    "        audio_buffer = []\n",
    "        silence_threshold = 500  # Ajustar conforme necess√°rio\n",
    "        silence_duration = 0\n",
    "        max_silence = 30  # 30 chunks de sil√™ncio antes de processar\n",
    "        \n",
    "        try:\n",
    "            while self.is_recording:\n",
    "                data = stream.read(self.chunk_size, exception_on_overflow=False)\n",
    "                audio_data = np.frombuffer(data, dtype=np.int16)\n",
    "                \n",
    "                # Detectar atividade de voz (VAD simples)\n",
    "                volume = np.sqrt(np.mean(audio_data**2))\n",
    "                \n",
    "                if volume > silence_threshold:\n",
    "                    audio_buffer.extend(audio_data)\n",
    "                    silence_duration = 0\n",
    "                else:\n",
    "                    silence_duration += 1\n",
    "                    if len(audio_buffer) > 0:\n",
    "                        audio_buffer.extend(audio_data)\n",
    "                \n",
    "                # Processar quando detectar pausa ou buffer cheio\n",
    "                if (silence_duration >= max_silence and len(audio_buffer) > 0) or len(audio_buffer) > self.sample_rate * 10:\n",
    "                    if len(audio_buffer) > self.sample_rate * 0.5:  # M√≠nimo 0.5 segundos\n",
    "                        self.audio_queue.put(np.array(audio_buffer))\n",
    "                    audio_buffer = []\n",
    "                    silence_duration = 0\n",
    "        \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro na grava√ß√£o: {e}\")\n",
    "        \n",
    "        finally:\n",
    "            stream.stop_stream()\n",
    "            stream.close()\n",
    "    \n",
    "    def _process_audio(self):\n",
    "        \"\"\"\n",
    "        Processa √°udio capturado para reconhecimento de fala\n",
    "        \"\"\"\n",
    "        self.is_processing = True\n",
    "        \n",
    "        while self.is_processing:\n",
    "            try:\n",
    "                # Pegar √°udio da fila (timeout para n√£o bloquear)\n",
    "                audio_data = self.audio_queue.get(timeout=1.0)\n",
    "                \n",
    "                # Converter para formato do speech_recognition\n",
    "                audio_bytes = (audio_data * 32767).astype(np.int16).tobytes()\n",
    "                \n",
    "                # Criar objeto AudioData\n",
    "                audio_sr = sr.AudioData(audio_bytes, self.sample_rate, 2)\n",
    "                \n",
    "                # Reconhecimento de fala\n",
    "                try:\n",
    "                    # Usar Google Speech Recognition (pode ser substitu√≠do por Whisper)\n",
    "                    text = self.recognizer.recognize_google(audio_sr, language='pt-BR')\n",
    "                    \n",
    "                    if text.strip():\n",
    "                        timestamp = datetime.now().strftime(\"%H:%M:%S\")\n",
    "                        self.text_queue.put({\n",
    "                            'text': text,\n",
    "                            'language': 'pt',\n",
    "                            'timestamp': timestamp,\n",
    "                            'confidence': 0.8  # Placeholder\n",
    "                        })\n",
    "                        print(f\"üó£Ô∏è [{timestamp}] Detectado: {text}\")\n",
    "                \n",
    "                except sr.UnknownValueError:\n",
    "                    pass  # N√£o conseguiu reconhecer\n",
    "                except sr.RequestError as e:\n",
    "                    print(f\"‚ùå Erro no servi√ßo de reconhecimento: {e}\")\n",
    "            \n",
    "            except queue.Empty:\n",
    "                continue\n",
    "            except Exception as e:\n",
    "                print(f\"‚ùå Erro no processamento: {e}\")\n",
    "    \n",
    "    def get_recognized_text(self):\n",
    "        \"\"\"\n",
    "        Retorna texto reconhecido da fila\n",
    "        \"\"\"\n",
    "        try:\n",
    "            return self.text_queue.get_nowait()\n",
    "        except queue.Empty:\n",
    "            return None\n",
    "    \n",
    "    def cleanup(self):\n",
    "        \"\"\"\n",
    "        Limpa recursos de √°udio\n",
    "        \"\"\"\n",
    "        self.stop_recording()\n",
    "        self.audio.terminate()\n",
    "\n",
    "# Teste do sistema de √°udio\n",
    "print(\"üéµ Classe AudioProcessor criada!\")\n",
    "print(\"üí° Use audio_processor.start_recording() para iniciar\")"
   ]
