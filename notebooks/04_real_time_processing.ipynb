    "class TextToSpeechEngine:\n",
    "    \"\"\"\n",
    "    Sistema de s√≠ntese de voz para tradu√ß√µes\n",
    "    \"\"\"\n",
    "    def __init__(self):\n",
    "        # Inicializar engines TTS\n",
    "        self.pyttsx_engine = None\n",
    "        self.temp_dir = \"../temp_audio\"\n",
    "        os.makedirs(self.temp_dir, exist_ok=True)\n",
    "        \n",
    "        # Configura√ß√µes\n",
    "        self.enabled = True\n",
    "        self.volume = 0.8\n",
    "        self.rate = 150  # palavras por minuto\n",
    "        \n",
    "        # Inicializar pygame para reprodu√ß√£o\n",
    "        try:\n",
    "            pygame.mixer.init()\n",
    "            print(\"üîä Pygame mixer inicializado!\")\n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Erro ao inicializar pygame: {e}\")\n",
    "        \n",
    "        # Inicializar pyttsx3\n",
    "        self._init_pyttsx()\n",
    "        \n",
    "        print(\"üó£Ô∏è Sistema TTS inicializado!\")\n",
    "    \n",
    "    def _init_pyttsx(self):\n",
    "        \"\"\"\n",
    "        Inicializa engine pyttsx3\n",
    "        \"\"\"\n",
    "        try:\n",
    "            self.pyttsx_engine = pyttsx3.init()\n",
    "            \n",
    "            # Configurar propriedades\n",
    "            self.pyttsx_engine.setProperty('rate', self.rate)\n",
    "            self.pyttsx_engine.setProperty('volume', self.volume)\n",
    "            \n",
    "            # Listar vozes dispon√≠veis\n",
    "            voices = self.pyttsx_engine.getProperty('voices')\n",
    "            if voices:\n",
    "                print(f\"üé§ Vozes dispon√≠veis: {len(voices)}\")\n",
    "                for i, voice in enumerate(voices[:3]):  # Mostrar apenas as 3 primeiras\n",
    "                    print(f\"  {i}: {voice.name} ({voice.languages})\")\n",
    "            \n",
    "            print(\"‚úÖ pyttsx3 configurado!\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Erro ao inicializar pyttsx3: {e}\")\n",
    "            self.pyttsx_engine = None\n",
    "    \n",
    "    def speak_text(self, text, language='en', method='pyttsx'):\n",
    "        \"\"\"\n",
    "        Converte texto em fala\n",
    "        \"\"\"\n",
    "        if not self.enabled or not text or not text.strip():\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            if method == 'pyttsx' and self.pyttsx_engine:\n",
    "                self._speak_pyttsx(text)\n",
    "            elif method == 'gtts':\n",
    "                self._speak_gtts(text, language)\n",
    "            else:\n",
    "                print(f\"üîä [TTS Simulado] {language.upper()}: {text}\")\n",
    "                \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro no TTS: {e}\")\n",
    "    \n",
    "    def _speak_pyttsx(self, text):\n",
    "        \"\"\"\n",
    "        Fala usando pyttsx3 (offline)\n",
    "        \"\"\"\n",
    "        def speak_thread():\n",
    "            try:\n",
    "                self.pyttsx_engine.say(text)\n",
    "                self.pyttsx_engine.runAndWait()\n",
    "            except Exception as e:\n",
    "                print(f\"‚ùå Erro pyttsx3: {e}\")\n",
    "        \n",
    "        # Executar em thread separada para n√£o bloquear\n",
    "        thread = threading.Thread(target=speak_thread)\n",
    "        thread.daemon = True\n",
    "        thread.start()\n",
    "    \n",
    "    def _speak_gtts(self, text, language):\n",
    "        \"\"\"\n",
    "        Fala usando Google TTS (online)\n",
    "        \"\"\"\n",
    "        def gtts_thread():\n",
    "            try:\n",
    "                # Mapear c√≥digos de idioma\n",
    "                lang_map = {'pt': 'pt-br', 'en': 'en'}\n",
    "                tts_lang = lang_map.get(language, 'en')\n",
    "                \n",
    "                # Gerar √°udio\n",
    "                tts = gTTS(text=text, lang=tts_lang, slow=False)\n",
    "                \n",
    "                # Salvar temporariamente\n",
    "                temp_file = os.path.join(self.temp_dir, f\"tts_{int(time.time())}.mp3\")\n",
    "                tts.save(temp_file)\n",
    "                \n",
    "                # Reproduzir\n",
    "                pygame.mixer.music.load(temp_file)\n",
    "                pygame.mixer.music.play()\n",
    "                \n",
    "                # Aguardar conclus√£o\n",
    "                while pygame.mixer.music.get_busy():\n",
    "                    time.sleep(0.1)\n",
    "                \n",
    "                # Limpar arquivo tempor√°rio\n",
    "                try:\n",
    "                    os.remove(temp_file)\n",
    "                except:\n",
    "                    pass\n",
    "                    \n",
    "            except Exception as e:\n",
    "                print(f\"‚ùå Erro gTTS: {e}\")\n",
    "        \n",
    "        # Executar em thread separada\n",
    "        thread = threading.Thread(target=gtts_thread)\n",
    "        thread.daemon = True\n",
    "        thread.start()\n",
    "    \n",
    "    def set_enabled(self, enabled):\n",
    "        \"\"\"\n",
    "        Habilita/desabilita TTS\n",
    "        \"\"\"\n",
    "        self.enabled = enabled\n",
    "        print(f\"üîä TTS {'habilitado' if enabled else 'desabilitado'}\")\n",
    "    \n",
    "    def set_volume(self, volume):\n",
