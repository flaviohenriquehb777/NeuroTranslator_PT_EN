    "    \n",
    "    def set_rate(self, rate):\n",
    "        \"\"\"\n",
    "        Ajusta velocidade da fala\n",
    "        \"\"\"\n",
    "        self.rate = max(50, min(300, rate))\n",
    "        if self.pyttsx_engine:\n",
    "            self.pyttsx_engine.setProperty('rate', self.rate)\n",
    "        print(f\"üó£Ô∏è Velocidade ajustada para: {self.rate} WPM\")\n",
    "    \n",
    "    def cleanup(self):\n",
    "        \"\"\"\n",
    "        Limpa recursos TTS\n",
    "        \"\"\"\n",
    "        try:\n",
    "            if self.pyttsx_engine:\n",
    "                self.pyttsx_engine.stop()\n",
    "            pygame.mixer.quit()\n",
    "        except:\n",
    "            pass\n",
    "\n",
    "# Criar engine TTS\n",
    "tts_engine = TextToSpeechEngine()\n",
