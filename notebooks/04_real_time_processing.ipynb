    "class RealTimeTranslator:\n",
    "    \"\"\"\n",
    "    Sistema de tradu√ß√£o em tempo real\n",
    "    \"\"\"\n",
    "    def __init__(self):\n",
    "        # Carregar modelos de tradu√ß√£o\n",
    "        self.models = {}\n",
    "        self.current_direction = 'pt_to_en'  # pt_to_en ou en_to_pt\n",
    "        \n",
    "        # Cache para tradu√ß√µes\n",
    "        self.translation_cache = {}\n",
    "        \n",
    "        # Fila de tradu√ß√µes\n",
    "        self.translation_queue = queue.Queue()\n",
    "        \n",
    "        # Inicializar modelos\n",
    "        self._load_models()\n",
    "        \n",
    "        print(\"üîÑ Sistema de tradu√ß√£o inicializado!\")\n",
    "    \n",
    "    def _load_models(self):\n",
    "        \"\"\"\n",
    "        Carrega modelos de tradu√ß√£o\n",
    "        \"\"\"\n",
    "        try:\n",
    "            # Modelo para PT -> EN\n",
    "            print(\"üì• Carregando modelo PT->EN...\")\n",
    "            self.models['pt_to_en'] = pipeline(\n",
    "                \"translation\",\n",
    "                model=\"unicamp-dl/translation-pt-en-t5\",\n",
    "                device=0 if torch.cuda.is_available() else -1\n",
    "            )\n",
    "            \n",
    "            # Modelo para EN -> PT\n",
    "            print(\"üì• Carregando modelo EN->PT...\")\n",
    "            self.models['en_to_pt'] = pipeline(\n",
    "                \"translation\",\n",
    "                model=\"unicamp-dl/translation-en-pt-t5\",\n",
    "                device=0 if torch.cuda.is_available() else -1\n",
    "            )\n",
    "            \n",
    "            print(\"‚úÖ Modelos carregados com sucesso!\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Erro ao carregar modelos: {e}\")\n",
    "            print(\"üîÑ Usando tradu√ß√£o simulada...\")\n",
    "            self.models = None\n",
    "    \n",
    "    def set_direction(self, direction):\n",
    "        \"\"\"\n",
    "        Define dire√ß√£o da tradu√ß√£o\n",
    "        \"\"\"\n",
    "        if direction in ['pt_to_en', 'en_to_pt']:\n",
    "            self.current_direction = direction\n",
    "            print(f\"üîÑ Dire√ß√£o alterada para: {direction}\")\n",
    "    \n",
    "    def translate_text(self, text, source_lang=None):\n",
    "        \"\"\"\n",
    "        Traduz texto usando o modelo apropriado\n",
    "        \"\"\"\n",
    "        if not text or not text.strip():\n",
    "            return \"\"\n",
    "        \n",
    "        # Verificar cache\n",
    "        cache_key = f\"{self.current_direction}:{text.strip().lower()}\"\n",
    "        if cache_key in self.translation_cache:\n",
    "            return self.translation_cache[cache_key]\n",
    "        \n",
    "        try:\n",
    "            if self.models and self.current_direction in self.models:\n",
    "                # Usar modelo real\n",
    "                model = self.models[self.current_direction]\n",
    "                result = model(text, max_length=512)\n",
    "                \n",
    "                if isinstance(result, list) and len(result) > 0:\n",
    "                    translation = result[0]['translation_text']\n",
    "                else:\n",
    "                    translation = str(result)\n",
    "            else:\n",
    "                # Tradu√ß√£o simulada\n",
    "                translation = self._simulate_translation(text)\n",
    "            \n",
    "            # Salvar no cache\n",
    "            self.translation_cache[cache_key] = translation\n",
    "            \n",
    "            return translation\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro na tradu√ß√£o: {e}\")\n",
    "            return f\"[Erro na tradu√ß√£o: {text}]\"\n",
    "    \n",
    "    def _simulate_translation(self, text):\n",
    "        \"\"\"\n",
    "        Simula√ß√£o de tradu√ß√£o para demonstra√ß√£o\n",
    "        \"\"\"\n",
    "        translations = {\n",
    "            'pt_to_en': {\n",
    "                'ol√°': 'hello',\n",
    "                'como voc√™ est√°': 'how are you',\n",
    "                'bom dia': 'good morning',\n",
    "                'boa tarde': 'good afternoon',\n",
    "                'boa noite': 'good evening',\n",
    "                'obrigado': 'thank you',\n",
    "                'por favor': 'please',\n",
    "                'com licen√ßa': 'excuse me',\n",
    "                'at√© logo': 'see you later',\n",
    "                'tchau': 'bye'\n",
    "            },\n",
    "            'en_to_pt': {\n",
    "                'hello': 'ol√°',\n",
    "                'how are you': 'como voc√™ est√°',\n",
    "                'good morning': 'bom dia',\n",
    "                'good afternoon': 'boa tarde',\n",
    "                'good evening': 'boa noite',\n",
    "                'thank you': 'obrigado',\n",
    "                'please': 'por favor',\n",
    "                'excuse me': 'com licen√ßa',\n",
    "                'see you later': 'at√© logo',\n",
    "                'bye': 'tchau'\n",
    "            }\n",
    "        }\n",
    "        \n",
    "        text_lower = text.lower().strip()\n",
    "        direction_dict = translations.get(self.current_direction, {})\n",
    "        \n",
    "        # Busca exata\n",
    "        if text_lower in direction_dict:\n",
    "            return direction_dict[text_lower]\n",
    "        \n",
    "        # Busca parcial\n",
    "        for key, value in direction_dict.items():\n",
    "            if key in text_lower:\n",
    "                return text.replace(key, value)\n",
    "        \n",
    "        # Tradu√ß√£o gen√©rica\n",
    "        if self.current_direction == 'pt_to_en':\n",
    "            return f\"[EN: {text}]\"\n",
    "        else:\n",
    "            return f\"[PT: {text}]\"\n",
    "    \n",
    "    def process_translation_queue(self, text_data):\n",
    "        \"\"\"\n",
    "        Processa fila de tradu√ß√µes\n",
    "        \"\"\"\n",
    "        if not text_data:\n",
    "            return None\n",
    "        \n",
    "        source_text = text_data['text']\n",
    "        source_lang = text_data.get('language', 'pt')\n",
    "        \n",
    "        # Determinar dire√ß√£o baseada no idioma detectado\n",
    "        if source_lang == 'pt':\n",
    "            self.set_direction('pt_to_en')\n",
    "        else:\n",
    "            self.set_direction('en_to_pt')\n",
    "        \n",
    "        # Traduzir\n",
    "        translation = self.translate_text(source_text)\n",
    "        \n",
    "        return {\n",
    "            'original': source_text,\n",
    "            'translation': translation,\n",
    "            'source_lang': source_lang,\n",
    "            'target_lang': 'en' if source_lang == 'pt' else 'pt',\n",
    "            'timestamp': text_data['timestamp'],\n",
    "            'confidence': text_data.get('confidence', 0.8)\n",
    "        }\n",
    "\n",
    "# Criar tradutor\n",
    "translator = RealTimeTranslator()\n",
    "print(\"üåê Sistema de tradu√ß√£o pronto!\")"
   ]
