    "class VideoProcessor:\n",
    "    \"\"\"\n",
    "    Sistema de captura e processamento de v√≠deo\n",
    "    \"\"\"\n",
    "    def __init__(self, camera_index=0):\n",
    "        self.camera_index = camera_index\n",
    "        self.cap = None\n",
    "        self.is_capturing = False\n",
    "        \n",
    "        # Configura√ß√µes de v√≠deo\n",
    "        self.frame_width = 640\n",
    "        self.frame_height = 480\n",
    "        self.fps = 30\n",
    "        \n",
    "        # Detec√ß√£o facial\n",
    "        self.face_cascade = None\n",
    "        self.known_faces = []\n",
    "        self.known_names = []\n",
    "        \n",
    "        # Filas para frames\n",
    "        self.frame_queue = queue.Queue(maxsize=10)\n",
    "        self.processed_frame_queue = queue.Queue(maxsize=5)\n",
    "        \n",
    "        # Inicializar detec√ß√£o facial\n",
    "        self._init_face_detection()\n",
    "        \n",
    "        print(\"üìπ Sistema de v√≠deo inicializado!\")\n",
    "    \n",
    "    def _init_face_detection(self):\n",
    "        \"\"\"\n",
    "        Inicializa sistema de detec√ß√£o facial\n",
    "        \"\"\"\n",
    "        try:\n",
    "            # Carregar classificador Haar Cascade\n",
    "            cascade_path = cv2.data.haarcascades + 'haarcascade_frontalface_default.xml'\n",
    "            self.face_cascade = cv2.CascadeClassifier(cascade_path)\n",
    "            \n",
    "            if self.face_cascade.empty():\n",
    "                print(\"‚ö†Ô∏è Erro ao carregar classificador facial\")\n",
    "                self.face_cascade = None\n",
    "            else:\n",
    "                print(\"‚úÖ Detec√ß√£o facial inicializada!\")\n",
    "                \n",
    "        except Exception as e:\n",
    "            print(f\"‚ö†Ô∏è Erro na inicializa√ß√£o facial: {e}\")\n",
    "            self.face_cascade = None\n",
    "    \n",
    "    def start_capture(self):\n",
    "        \"\"\"\n",
    "        Inicia captura de v√≠deo\n",
    "        \"\"\"\n",
    "        if self.is_capturing:\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            # Inicializar c√¢mera\n",
    "            self.cap = cv2.VideoCapture(self.camera_index)\n",
    "            \n",
    "            if not self.cap.isOpened():\n",
    "                print(f\"‚ùå N√£o foi poss√≠vel abrir c√¢mera {self.camera_index}\")\n",
    "                return False\n",
    "            \n",
    "            # Configurar propriedades\n",
    "            self.cap.set(cv2.CAP_PROP_FRAME_WIDTH, self.frame_width)\n",
    "            self.cap.set(cv2.CAP_PROP_FRAME_HEIGHT, self.frame_height)\n",
    "            self.cap.set(cv2.CAP_PROP_FPS, self.fps)\n",
    "            \n",
    "            self.is_capturing = True\n",
    "            \n",
    "            # Thread para captura\n",
    "            self.capture_thread = threading.Thread(target=self._capture_frames)\n",
    "            self.capture_thread.daemon = True\n",
    "            self.capture_thread.start()\n",
    "            \n",
    "            # Thread para processamento\n",
    "            self.processing_thread = threading.Thread(target=self._process_frames)\n",
    "            self.processing_thread.daemon = True\n",
    "            self.processing_thread.start()\n",
    "            \n",
    "            print(\"üìπ Captura de v√≠deo iniciada!\")\n",
    "            return True\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao iniciar captura: {e}\")\n",
    "            return False\n",
    "    \n",
    "    def stop_capture(self):\n",
    "        \"\"\"\n",
    "        Para captura de v√≠deo\n",
    "        \"\"\"\n",
    "        self.is_capturing = False\n",
    "        \n",
    "        if self.cap:\n",
    "            self.cap.release()\n",
    "            self.cap = None\n",
    "        \n",
    "        print(\"‚èπÔ∏è Captura de v√≠deo parada!\")\n",
    "    \n",
    "    def _capture_frames(self):\n",
    "        \"\"\"\n",
    "        Captura frames da c√¢mera\n",
    "        \"\"\"\n",
    "        while self.is_capturing and self.cap:\n",
    "            try:\n",
    "                ret, frame = self.cap.read()\n",
    "                \n",
    "                if ret:\n",
    "                    # Adicionar timestamp\n",
    "                    timestamp = time.time()\n",
    "                    \n",
    "                    # Adicionar √† fila (descartar se cheia)\n",
    "                    try:\n",
    "                        self.frame_queue.put_nowait({\n",
    "                            'frame': frame,\n",
    "                            'timestamp': timestamp\n",
    "                        })\n",
    "                    except queue.Full:\n",
    "                        # Descartar frame mais antigo\n",
    "                        try:\n",
    "                            self.frame_queue.get_nowait()\n",
    "                            self.frame_queue.put_nowait({\n",
    "                                'frame': frame,\n",
    "                                'timestamp': timestamp\n",
    "                            })\n",
    "                        except queue.Empty:\n",
    "                            pass\n",
    "                else:\n",
    "                    print(\"‚ö†Ô∏è Falha na captura do frame\")\n",
    "                    break\n",
    "                    \n",
    "            except Exception as e:\n",
    "                print(f\"‚ùå Erro na captura: {e}\")\n",
    "                break\n",
    "    \n",
    "    def _process_frames(self):\n",
    "        \"\"\"\n",
    "        Processa frames capturados\n",
    "        \"\"\"\n",
    "        while self.is_capturing:\n",
    "            try:\n",
    "                # Pegar frame da fila\n",
    "                frame_data = self.frame_queue.get(timeout=1.0)\n",
    "                frame = frame_data['frame']\n",
    "                timestamp = frame_data['timestamp']\n",
    "                \n",
    "                # Processar frame\n",
    "                processed_frame = self._detect_faces(frame)\n",
    "                \n",
    "                # Adicionar √† fila de frames processados\n",
    "                try:\n",
    "                    self.processed_frame_queue.put_nowait({\n",
    "                        'frame': processed_frame,\n",
    "                        'timestamp': timestamp,\n",
    "                        'original': frame\n",
    "                    })\n",
    "                except queue.Full:\n",
    "                    # Descartar frame mais antigo\n",
    "                    try:\n",
    "                        self.processed_frame_queue.get_nowait()\n",
    "                        self.processed_frame_queue.put_nowait({\n",
    "                            'frame': processed_frame,\n",
    "                            'timestamp': timestamp,\n",
    "                            'original': frame\n",
    "                        })\n",
    "                    except queue.Empty:\n",
    "                        pass\n",
    "                        \n",
    "            except queue.Empty:\n",
    "                continue\n",
    "            except Exception as e:\n",
    "                print(f\"‚ùå Erro no processamento: {e}\")\n",
    "    \n",
    "    def _detect_faces(self, frame):\n",
    "        \"\"\"\n",
    "        Detecta faces no frame\n",
    "        \"\"\"\n",
    "        if self.face_cascade is None:\n",
    "            return frame\n",
    "        \n",
    "        try:\n",
    "            # Converter para escala de cinza\n",
    "            gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n",
    "            \n",
    "            # Detectar faces\n",
    "            faces = self.face_cascade.detectMultiScale(\n",
    "                gray,\n",
    "                scaleFactor=1.1,\n",
    "                minNeighbors=5,\n",
    "                minSize=(30, 30)\n",
    "            )\n",
    "            \n",
    "            # Desenhar ret√¢ngulos nas faces\n",
    "            for (x, y, w, h) in faces:\n",
    "                cv2.rectangle(frame, (x, y), (x+w, y+h), (0, 255, 0), 2)\n",
    "                \n",
    "                # Adicionar label\n",
    "                cv2.putText(frame, 'Face Detectada', (x, y-10),\n",
    "                           cv2.FONT_HERSHEY_SIMPLEX, 0.5, (0, 255, 0), 1)\n",
    "            \n",
    "            return frame\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro na detec√ß√£o facial: {e}\")\n",
    "            return frame\n",
    "    \n",
    "    def get_latest_frame(self):\n",
    "        \"\"\"\n",
    "        Retorna o frame mais recente processado\n",
    "        \"\"\"\n",
    "        try:\n",
    "            return self.processed_frame_queue.get_nowait()\n",
    "        except queue.Empty:\n",
    "            return None\n",
    "    \n",
    "    def add_known_face(self, image_path, name):\n",
    "        \"\"\"\n",
    "        Adiciona face conhecida para reconhecimento\n",
    "        \"\"\"\n",
    "        try:\n",
    "            # Carregar imagem\n",
    "            image = face_recognition.load_image_file(image_path)\n",
    "            encoding = face_recognition.face_encodings(image)[0]\n",
    "            \n",
    "            self.known_faces.append(encoding)\n",
    "            self.known_names.append(name)\n",
    "            \n",
    "            print(f\"‚úÖ Face de {name} adicionada!\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao adicionar face: {e}\")\n",
    "    \n",
    "    def cleanup(self):\n",
    "        \"\"\"\n",
    "        Limpa recursos de v√≠deo\n",
    "        \"\"\"\n",
    "        self.stop_capture()\n",
    "        cv2.destroyAllWindows()\n",
    "\n",
    "# Criar processador de v√≠deo\n",
    "video_processor = VideoProcessor()\n",
    "print(\"üì∏ Sistema de v√≠deo pronto!\")"
   ]
